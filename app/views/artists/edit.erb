<h1>Edit Artist</h1>


<form action="/artists/<%= @artist.id %>" method="POST">
 <input id="hidden" type="hidden" name="_method" value="PATCH">
 <label for="name">Artist Name</label><br>
 <input type="text" name="name" value="<%= @artist.name %>">
 <br><br>

 <label for="hometown">Hometown</label><br>
 <input type="text" name="hometown" value="<%= @artist.hometown %>"><br><br>

 <button type="submit">Edit</button>
 
</form>